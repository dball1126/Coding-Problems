    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:Coding Problems}/swift",
      "name": "Release swift (swift)",
      "program": "${workspaceFolder:Coding Problems}/swift/.build/release/swift",
      "preLaunchTask": "swift: Build Release swift (swift)"
    }
  ]
